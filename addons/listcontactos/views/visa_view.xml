<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Vista formulario para Visa Type -->

    <record model = "ir.ui.view" id = "visa_form_view">
            <field name="name">visa.form.view</field>
            <field name="model">catalog.visa_type</field>
            <field name="arch" type="xml">
                <form string="Visa Type">
                    <sheet>
                        <group>
                            <field name="name" />
                            <field name="description" />
                        </group>
                    </sheet>
                </form>
            </field>
    </record>

    <!-- Vista Arbol (Tree) para Visa Type -->

    <record model = "ir.ui.view" id = "visa_tree_view">
            <field name="name">visa.tree.view</field>
            <field name="model">catalog.visa_type</field>
            <field name="arch" type="xml">
                <tree string="Visa Type">
                    <field name="name" />
                    <field name="description" />
                </tree>
            </field>
    </record>


</odoo>